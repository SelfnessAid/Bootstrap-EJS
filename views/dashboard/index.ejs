    <script>
        $("#small-bot-say").click(function() {
            var $me = $("#small-bot-say");
            var $target = $("#expanded-bot-say");
            $me.hide("slow");
            $target.show("slide", {direction:"right"}, "slow");            
        });
    
        $("#close-expanded-botview").click(function() {
            var $me = $("#small-bot-say");
            var $target = $("#expanded-bot-say");
